<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="at.ac.univie.countagram.activity.AboutusActivity">

    <Button
        android:id="@+id/homeButton"
        style="@android:style/Widget.Button"
        android:layout_width="52dp"
        android:layout_height="42dp"
        android:background="?attr/colorButtonNormal"
        android:onClick="homeOnClick"
        android:text="Home"
        android:textAllCaps="true"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.993"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.965" />

    <TextView
        android:id="@+id/teamDescriptionTxt"
        android:layout_width="230dp"
        android:layout_height="83dp"
        android:fontFamily="sans-serif"
        android:text="We are four computer science students, and we want to support you with your personal nutrition goal!"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.46" />

    <TextView
        android:id="@+id/teamMember4"
        android:layout_width="324dp"
        android:layout_height="25dp"
        android:fontFamily="sans-serif"
        android:text="Daniel Tircob, BSc in Scientific Computing"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.794" />

    <TextView
        android:id="@+id/teamMember3"
        android:layout_width="324dp"
        android:layout_height="25dp"
        android:fontFamily="sans-serif"
        android:text="Lisa Schierer, BSc in Media Informatics"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.742" />

    <TextView
        android:id="@+id/teamMember2"
        android:layout_width="324dp"
        android:layout_height="25dp"
        android:fontFamily="sans-serif"
        android:text="Viktoriya Lazarova, BSc in Media Informatics"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.691" />

    <TextView
        android:id="@+id/teamMember1"
        android:layout_width="324dp"
        android:layout_height="25dp"
        android:fontFamily="sans-serif"
        android:text="Cordula Eggerth, BSc in Business Informatics"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="?attr/actionMenuTextColor"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.639" />

    <TextView
        android:id="@+id/teamTxt"
        android:layout_width="262dp"
        android:layout_height="32dp"
        android:fontFamily="sans-serif"
        android:text="The Count-A-Gram Team"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.311" />

    <ImageView
        android:id="@+id/countagramLogo"
        android:layout_width="452dp"
        android:layout_height="132dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/ic_launcher" />

</android.support.constraint.ConstraintLayout>
